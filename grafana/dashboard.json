{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "victoriametrics-datasource",
      "pluginName": "VictoriaMetrics"
    }
  ],
  "__elements": {},
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.0.0" },
    { "type": "datasource", "id": "victoriametrics-datasource", "name": "VictoriaMetrics", "version": "1.0.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "" },
    { "type": "panel", "id": "bargauge", "name": "Bar gauge", "version": "" },
    { "type": "panel", "id": "table", "name": "Table", "version": "" },
    { "type": "panel", "id": "heatmap", "name": "Heatmap", "version": "" }
  ],
  "id": null,
  "uid": "arcrun-overview",
  "title": "ArcRun",
  "description": "Task orchestration service monitoring",
  "tags": ["arcrun", "prometheus"],
  "style": "dark",
  "timezone": "browser",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "liveNow": false,
  "refresh": "30s",
  "schemaVersion": 39,
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "7d"]
  },
  "templating": {
    "list": [
      {
        "current": {},
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "definition": "label_values(tasks_completed_total, kind)",
        "name": "kind",
        "label": "Kind",
        "type": "query",
        "includeAll": true,
        "multi": true,
        "allValue": ".*",
        "refresh": 2,
        "sort": 1
      }
    ]
  },
  "panels": [
    {
      "title": "Overview",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Tasks by Status",
      "type": "bargauge",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          },
          "color": { "mode": "fixed", "fixedColor": "blue" }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Running" }, "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "#3498db" } }] },
          { "matcher": { "id": "byName", "options": "Pending" }, "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "#f39c12" } }] },
          { "matcher": { "id": "byName", "options": "Waiting" }, "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "#666666" } }] },
          { "matcher": { "id": "byName", "options": "Success" }, "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "#27ae60" } }] },
          { "matcher": { "id": "byName", "options": "Failure" }, "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "#e74c3c" } }] },
          { "matcher": { "id": "byName", "options": "Canceled" }, "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "#95a5a6" } }] },
          { "matcher": { "id": "byName", "options": "Paused" }, "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "#9b59b6" } }] },
          { "matcher": { "id": "byName", "options": "Claimed" }, "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "#1abc9c" } }] }
        ]
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true,
        "minVizWidth": 8,
        "minVizHeight": 16
      },
      "targets": [
        {
          "expr": "tasks_by_status",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Task Throughput",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 15, "pointSize": 5, "lineWidth": 2 },
          "unit": "ops"
        },
        "overrides": []
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "rate(tasks_created_total[5m])",
          "legendFormat": "Created",
          "refId": "A"
        },
        {
          "expr": "sum(rate(tasks_completed_total{kind=~\"$kind\"}[5m]))",
          "legendFormat": "Completed",
          "refId": "B"
        },
        {
          "expr": "rate(tasks_cancelled_total[5m])",
          "legendFormat": "Cancelled",
          "refId": "C"
        },
        {
          "expr": "rate(tasks_timed_out_total[5m])",
          "legendFormat": "Timed Out",
          "refId": "D"
        }
      ]
    },
    {
      "title": "Failure Rate",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 3, "w": 4, "x": 16, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.05 },
              { "color": "red", "value": 0.15 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background", "graphMode": "area" },
      "targets": [
        {
          "expr": "sum(rate(tasks_completed_total{outcome=\"failure\"}[5m])) / clamp_min(sum(rate(tasks_completed_total[5m])), 1)",
          "legendFormat": "Failure %",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Timeouts",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 3, "w": 4, "x": 20, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 10 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background", "graphMode": "area" },
      "targets": [
        {
          "expr": "increase(tasks_timed_out_total[1h])",
          "legendFormat": "Last hour",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Running Tasks by Kind",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 3, "w": 4, "x": 16, "y": 4 },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 30, "stacking": { "mode": "normal" }, "lineWidth": 1 }
        },
        "overrides": []
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "running_tasks_by_kind{kind=~\"$kind\"}",
          "legendFormat": "{{kind}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Completed by Kind",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 3, "w": 4, "x": 20, "y": 4 },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "bars", "fillOpacity": 80, "stacking": { "mode": "normal" }, "lineWidth": 0 },
          "unit": "ops"
        },
        "overrides": []
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum by (kind) (rate(tasks_completed_total{kind=~\"$kind\"}[5m]))",
          "legendFormat": "{{kind}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Latency",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 7 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Task Execution Duration (p50 / p95 / p99)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 8 },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2 },
          "unit": "s"
        },
        "overrides": []
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "max"] } },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(task_duration_seconds_bucket{kind=~\"$kind\"}[5m])))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(task_duration_seconds_bucket{kind=~\"$kind\"}[5m])))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(task_duration_seconds_bucket{kind=~\"$kind\"}[5m])))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ]
    },
    {
      "title": "Wait Time Pending → Running (p50 / p95)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 8 },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2 },
          "unit": "s"
        },
        "overrides": []
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "max"] } },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(task_wait_seconds_bucket{kind=~\"$kind\"}[5m])))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(task_wait_seconds_bucket{kind=~\"$kind\"}[5m])))",
          "legendFormat": "p95",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Task Duration by Kind (p95)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 15 },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2 },
          "unit": "s"
        },
        "overrides": []
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, kind) (rate(task_duration_seconds_bucket{kind=~\"$kind\"}[5m])))",
          "legendFormat": "{{kind}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Status Transitions",
      "type": "table",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 15 },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "options": { "showHeader": true, "footer": { "show": false } },
      "transformations": [
        { "id": "sortBy", "options": { "fields": {}, "sort": [{ "field": "Value", "desc": true }] } }
      ],
      "targets": [
        {
          "expr": "increase(task_status_transitions_total[1h]) > 0",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Webhooks",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 22 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Webhook Execution Rate",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 7, "w": 8, "x": 0, "y": 23 },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 15, "lineWidth": 2 },
          "unit": "ops"
        },
        "overrides": []
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum by (trigger, outcome) (rate(webhook_executions_total[5m]))",
          "legendFormat": "{{trigger}} / {{outcome}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Webhook Duration (p95)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 7, "w": 8, "x": 8, "y": 23 },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2 },
          "unit": "s"
        },
        "overrides": []
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, trigger) (rate(webhook_duration_seconds_bucket[5m])))",
          "legendFormat": "p95 {{trigger}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.50, sum by (le, trigger) (rate(webhook_duration_seconds_bucket[5m])))",
          "legendFormat": "p50 {{trigger}}",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Webhook Idempotency",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 7, "w": 8, "x": 16, "y": 23 },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 15, "lineWidth": 2 },
          "unit": "ops"
        },
        "overrides": []
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum by (trigger) (rate(webhook_idempotent_skips_total[5m]))",
          "legendFormat": "Skipped ({{trigger}})",
          "refId": "A"
        },
        {
          "expr": "rate(webhook_idempotent_conflicts_total[5m])",
          "legendFormat": "Conflicts",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Dependencies & Concurrency",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 30 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Dependency Propagation",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 7, "w": 8, "x": 0, "y": 31 },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 15, "lineWidth": 2 },
          "unit": "ops"
        },
        "overrides": []
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum by (parent_outcome) (rate(dependency_propagations_total[5m]))",
          "legendFormat": "{{parent_outcome}}",
          "refId": "A"
        },
        {
          "expr": "rate(tasks_unblocked_total[5m])",
          "legendFormat": "Unblocked",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Failures by Dependency & Dead-end",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 7, "w": 8, "x": 8, "y": 31 },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 15, "lineWidth": 2 },
          "unit": "ops",
          "color": { "mode": "palette-classic" }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Failed by dep" }, "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }] },
          { "matcher": { "id": "byName", "options": "Dead-end canceled" }, "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "orange" } }] }
        ]
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "rate(tasks_failed_by_dependency_total[5m])",
          "legendFormat": "Failed by dep",
          "refId": "A"
        },
        {
          "expr": "rate(tasks_canceled_dead_end_total[5m])",
          "legendFormat": "Dead-end canceled",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Concurrency Blocks",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 7, "w": 8, "x": 16, "y": 31 },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 20, "lineWidth": 2 },
          "unit": "ops",
          "color": { "mode": "fixed", "fixedColor": "yellow" }
        },
        "overrides": []
      },
      "options": { "tooltip": { "mode": "single" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "rate(tasks_blocked_by_concurrency_total[5m])",
          "legendFormat": "Blocked/s",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Worker Loop",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 38 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Worker Loop Duration",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 7, "w": 8, "x": 0, "y": 39 },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2 },
          "unit": "s"
        },
        "overrides": []
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(worker_loop_duration_seconds_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(worker_loop_duration_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(worker_loop_duration_seconds_bucket[5m]))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ]
    },
    {
      "title": "Tasks Processed per Loop",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 7, "w": 8, "x": 8, "y": 39 },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 20, "lineWidth": 2 }
        },
        "overrides": []
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(tasks_processed_per_loop_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(tasks_processed_per_loop_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Batch & DB Save Failures",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 7, "w": 8, "x": 16, "y": 39 },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background", "graphMode": "area", "textMode": "auto" },
      "targets": [
        {
          "expr": "increase(batch_update_failures_total[1h])",
          "legendFormat": "Batch update failures",
          "refId": "A"
        },
        {
          "expr": "increase(tasks_db_save_failures_total[1h])",
          "legendFormat": "DB save failures",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Database",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 46 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "DB Query Duration (p95 by query)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 47 },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2 },
          "unit": "s"
        },
        "overrides": []
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "max"] } },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, query) (rate(db_query_duration_seconds_bucket[5m])))",
